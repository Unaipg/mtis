          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="makeBookingResponse">
      	<xsd:complexType>
      		<xsd:sequence>
      			<xsd:element name="out" type="xsd:boolean"/>
      		</xsd:sequence>
      	</xsd:complexType>
      </xsd:element>
      
      <xsd:complexType name="Hotel">
      	<xsd:attribute name="id" type="xsd:int"/>
      	<xsd:attribute name="name" type="xsd:string"/>
      </xsd:complexType>
      <xsd:complexType name="Hotels">
        <xsd:sequence>
        	<xsd:element maxOccurs="unbounded" name="item" type="tns:Hotel"/>
        </xsd:sequence>
      </xsd:complexType>
      
      <xsd:complexType name="Room">
      	<xsd:attribute name="id" type="xsd:int"/>
