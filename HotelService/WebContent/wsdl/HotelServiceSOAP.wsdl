      <xsd:element name="getHotels">
        <xsd:complexType>
          <xsd:sequence>
          	<xsd:element name="in" type="xsd:string"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="getHotelsResponse">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="out" type="tns:Hotels"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="getRooms">
        <xsd:complexType>
          <xsd:sequence>
